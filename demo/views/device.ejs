<!DOCTYPE html>
<html>
    <%-include("./templates/pageHead",{title: title})%>
    <body>
        <h1>
            <%= title %>
        </h1>
     
        <%-include("./templates/pageFoot")%>
        <button onclick="getBrowser()">getBrowser</button>
        <button onclick="getOS()">getOS</button>
        <p id="browser"></p>
        <p id="os"></p>
        <script>
            function getBrowser(params) {
                var data = lumos.getBrowser()
                console.log(data);
                
                document.querySelector("#browser").innerHTML = JSON.stringify(data)
                
            }
            function getOS(params) {
                var data = lumos.getOS()
                console.log(data);
                
                document.querySelector("#os").innerHTML = JSON.stringify(data)
                
            }
        </script>
    </body>
</html>